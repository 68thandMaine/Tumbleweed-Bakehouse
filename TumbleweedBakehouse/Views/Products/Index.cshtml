@{
  Layout = "_Layout";
}
<div class='row'>

  <div class='col-md-4'>
    <h3>Orders</h3>

    @if (Model.Count != 0)
    {
      <ul>
        @foreach (var product in Model)
        {
          <li>
            <a href='/products/@product.GetId()'>@product.GetProductName()</a>
          </li>
        }
      </ul>
    }
  </div>

</div>
