@{
  Layout = "_Layout";
}
<div class='row'>
  <div class='col-md-4'>
    <h3>Orders</h3>

    @if (Model.Count != 0)
    {
      <ul>
        @foreach (var order in Model)
        {
          <li><a href='/orders/@order.GetId()'>@order.GetOrderNumber() :: @order.GetPickUpLocation() :: @order.GetPickUpDate()</a></li>
        }
      </ul>
    }
  </div>
</div>
