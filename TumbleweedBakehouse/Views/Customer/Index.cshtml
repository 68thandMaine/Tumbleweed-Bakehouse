@{
  Layout = "_Layout";
}
<div class='row'>
  <div class='col-md-12'>
      <h3>Customers</h3>

      @if (Model.Count != 0)
      {
        <ul>
          @foreach (var customer in Model)
          {
            <li>
              <a href='/customer/@customer.GetId()'>@customer.GetFirstLast()</a>
            </li>
          }
        </ul>
      }
  </div>
</div>
