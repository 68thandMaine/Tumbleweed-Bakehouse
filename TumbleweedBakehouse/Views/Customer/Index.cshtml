@{
  Layout = "_Layout";
}
<div class='row'>
  <div class='col-md-12'>
      <h3>Orders</h3>

      @if (Model.Count != 0)
      {
        <ul>
          @foreach (var customer in Model)
          {
            <li><a href='/customers/@customer.GetId()'>@customer.GetFirstLast()</a></li>
          }
        </ul>
      }
  </div>
</div>
